<div class="author">
    <h1 class="author_heading">Search Results</h1>
</div>
<section class="articles">
    <ul class="article-ul">
        {{#each data}}
            <li>
                <a href="/post/{{_id}}">
                    <span>{{title}}</span>
                    <span class="article-list__date">{{createdAt.toDateString}}</span>
                </a>
            </li>
        {{/each}}
    </ul>
</section>
